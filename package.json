{
  "name": "@convex-dev/launchdarkly",
  "description": "A Convex component that syncs LaunchDarkly feature flags.",
  "homepage": "https://github.com/get-convex/launchdarkly#readme",
  "version": "0.4.0",
  "type": "module",
  "repository": "github:get-convex/launchdarkly",
  "bugs": {
    "email": "support@convex.dev",
    "url": "https://github.com/get-convex/launchdarkly"
  },
  "license": "Apache-2.0",
  "keywords": [
    "convex",
    "launchdarkly",
    "component",
    "feature-flags"
  ],
  "scripts": {
    "dev": "run-p -r 'dev:*'",
    "dev:backend": "convex dev --typecheck-components --run example:seedData",
    "dev:frontend": "cd example && vite --clearScreen false",
    "dev:build": "chokidar 'tsconfig*.json' 'src/**/*.ts' -i '**/*.test.ts' -c 'convex codegen --component-dir ./src/component && npm run build' --initial",
    "predev": "path-exists .env.local || (convex dev --skip-push && npm run build)",
    "clean": "rm -rf dist *.tsbuildinfo",
    "build": "tsc --project ./tsconfig.build.json && ./scripts/replace-version.sh .",
    "typecheck": "tsc --noEmit && tsc -p example && tsc -p example/convex",
    "lint": "eslint .",
    "all": "run-p -r 'dev:*' 'test:watch'",
    "test": "vitest run --typecheck",
    "test:watch": "vitest --typecheck --clearScreen false",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "test:coverage": "vitest run --coverage --coverage.reporter=text",
    "prepublishOnly": "npm run clean && npm run build",
    "preversion": "npm run clean && npm ci && npm run build && run-p test lint typecheck",
    "alpha": "npm version prerelease --preid alpha && npm publish --tag alpha && git push --follow-tags",
    "release": "npm version patch && npm publish && git push --follow-tags && git push",
    "version": "vim -c 'normal o' -c 'normal o## '$npm_package_version CHANGELOG.md && prettier -w CHANGELOG.md && git add CHANGELOG.md"
  },
  "files": [
    "dist",
    "src"
  ],
  "peerDependencies": {
    "convex": "^1.24.8"
  },
  "dependencies": {
    "@launchdarkly/js-server-sdk-common": "2.8.0",
    "crypto-js": "^4.2.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@edge-runtime/vm": "5.0.0",
    "@eslint/js": "^9.9.1",
    "@fast-check/vitest": "^0.1.3",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.5.8",
    "@types/node": "20.19.24",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^2.1.1",
    "chokidar-cli": "3.0.0",
    "convex": "1.29.0",
    "convex-test": "^0.0.35",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^15.9.0",
    "npm-run-all": "^4.1.5",
    "npm-run-all2": "8.0.4",
    "path-exists-cli": "2.0.0",
    "prettier": "3.2.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.4.0",
    "vite": "^7.1.12",
    "vitest": "^2.1.1"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./test": "./src/test.ts",
    "./_generated/component.js": {
      "types": "./dist/component/_generated/component.d.ts"
    },
    "./convex.config": {
      "types": "./dist/component/convex.config.d.ts",
      "default": "./dist/component/convex.config.js"
    },
    "./convex.config.js": {
      "types": "./dist/component/convex.config.d.ts",
      "default": "./dist/component/convex.config.js"
    }
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js"
}
